# Multi-host deployment configuration
DEPLOYMENT_MODE=multihost

# Ollama Configuration
OLLAMA_HOST=http://ollama:11434

# GitHub MCP Server
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here

# Context7 MCP Server (Upstash Redis)
UPSTASH_REDIS_REST_URL=your_upstash_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_token_here

# HIL Settings
HIL_ENABLED=true
HIL_TIMEOUT_MS=30000

# Compatible Models (no detection needed)
COMPATIBLE_MODELS=qwen2.5,qwen3,llama3.1,llama3.2,mistral,deepseek-r1

# Database Configuration
MONGO_URI=mongodb://mongodb:27017/olympian-ai
REDIS_URL=redis://redis:6379

# Server Configuration
PORT=3000
NODE_ENV=production
